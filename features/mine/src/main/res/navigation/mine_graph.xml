<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mine"
    app:startDestination="@id/mineFragment"
    tools:ignore="UnusedNavigation">

    <fragment
            android:id="@+id/mineFragment"
            android:name="com.leevinapp.monitor.mine.ui.MineFragment"
            android:label="@string/mine"
            tools:layout="@layout/mine_fragment">
        <action
            android:id="@+id/mine_action_minefragment_to_mineaboutfragment"
            app:destination="@id/mine_mineaboutfragment" />
        <action
            android:id="@+id/mine_action_minefragment_to_minesecurityfragment"
            app:destination="@id/mine_minesecurityfragment" />
        <action
            android:id="@+id/mine_action_minefragment_to_minepersonalinfofragment"
            app:destination="@id/mine_minepersonalinfofragment" />
        <action
            android:id="@+id/mine_action_minefragment_to_mineauthfragment"
            app:destination="@id/mine_mineauthfragment" />
    </fragment>

    <include app:graph="@navigation/auth_graph" />

    <fragment
        android:id="@+id/mine_mineaboutfragment"
        android:name="com.leevinapp.monitor.mine.ui.MineAboutFragment"
        android:label="MineAboutFragment"
        tools:layout="@layout/mine_frament_about"/>
    <fragment
        android:id="@+id/mine_minesecurityfragment"
        android:name="com.leevinapp.monitor.mine.ui.security.MineSecurityFragment"
        android:label="MineSecurityFragment"
        tools:layout="@layout/mine_fragment_security">
        <action
            android:id="@+id/mine_action_mine_minesecurityfragment_to_mineforgotpasswordfragment"
            app:destination="@id/mine_mineforgotpasswordfragment" />
        <action
            android:id="@+id/mine_action_mine_minesecurityfragment_to_minechangesecurityinfofragment"
            app:destination="@id/mine_minechangesecurityinfofragment" />
        <action
            android:id="@+id/mine_action_mine_minesecurityfragment_to_mine_minechangepasswordfragment"
            app:destination="@id/mine_minechangepasswordfragment" />
    </fragment>
    <fragment
        android:id="@+id/mine_minepersonalinfofragment"
        android:name="com.leevinapp.monitor.mine.ui.MinePersonalInfoFragment"
        android:label="MinePersonalInfoFragment"
        tools:layout="@layout/mine_fragment_personal_info"/>
    <fragment
        android:id="@+id/mine_mineauthfragment"
        android:name="com.leevinapp.monitor.mine.ui.identityauth.MineIdentityAuthFragment"
        android:label="MineAuthFragment"
        tools:layout="@layout/mine_fragment_auth">
        <argument
            android:name="authModel"
            app:argType="com.leevinapp.monitor.mine.domain.model.OptionModel"
            app:nullable="false"/>
    </fragment>
    <fragment
        android:id="@+id/mine_minechangepasswordfragment"
        android:name="com.leevinapp.monitor.mine.ui.security.MineChangePasswordFragment"
        android:label="MineChangePasswordFragment"
        tools:layout="@layout/mine_fragment_change_password"/>
    <fragment
        android:id="@+id/mine_minechangesecurityinfofragment"
        android:name="com.leevinapp.monitor.mine.ui.security.MineChangeSecurityInfoFragment"
        android:label="MineChangeSecurityInfoFragment"
        tools:layout="@layout/mine_fragment_change_security_info"/>
    <fragment
        android:id="@+id/mine_mineforgotpasswordfragment"
        android:name="com.leevinapp.monitor.mine.ui.security.MineForgotPasswordFragment"
        android:label="MineForgotPasswordFragment"
        tools:layout="@layout/mine_fragment_forgot_password">
        <action
            android:id="@+id/mine_action_mine_mineforgotpasswordfragment_to_mineresetpasswordfragment"
            app:destination="@id/mine_mineresetpasswordfragment" />
    </fragment>
    <fragment
        android:id="@+id/mine_mineresetpasswordfragment"
        android:name="com.leevinapp.monitor.mine.ui.security.MineResetPasswordFragment"
        android:label="MineResetPasswordFragment"
        tools:layout="@layout/mine_fragment_reset_password"/>

</navigation>
